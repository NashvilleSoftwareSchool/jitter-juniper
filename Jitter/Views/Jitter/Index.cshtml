@model IEnumerable<Jitter.Models.Jot>
@{
    Layout = "~/Views/Shared/_JitterLayout.cshtml";
}

@foreach (var item in Model)
{
    <div class="row">
        <div class="col-lg-12">
            <div class="jot">
                @item.Content
            </div>
        </div>
    </div>
}

